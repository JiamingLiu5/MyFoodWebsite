# Copy this file to .env and fill values before running

# Cloudflare R2 / S3-compatible settings
R2_ENDPOINT=https://<accountid>.r2.cloudflarestorage.com
R2_BUCKET=your-bucket-name
R2_ACCESS_KEY_ID=AKIAEXAMPLE
R2_SECRET_ACCESS_KEY=your-secret-key
R2_PUBLIC_BASE_URL=https://<bucket>.<accountid>.r2.cloudflarestorage.com

# Session secret (use a strong random value)
SESSION_SECRET=change_this_to_a_long_random_secret

# App port (optional)
PORT=3000
